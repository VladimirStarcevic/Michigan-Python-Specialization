    "def addit(num: int | float) -> int | float:\n",
    "    return num + 5\n",
    "\n",
    "def mult(num: int | float) -> int | float:\n",
    "    result = addit(num) * num\n",
    "    return result\n",
    "\n",
    "output = mult(10)\n",
    "print(f\"Result  is : {output}\")"
